{
    "size":{{size}}{{^size}}10{{/size}},
    {{#from}}"from":{{from}},{{/from}}
    "aggregations": {
        "brand": {
            "terms": {
                "field": "brand"
            }
        },
        "min_price": {
            "min": {
                "field": "price"
            }
        }
        ,
        "max_price": {
            "max": {
                "field": "price"
            }
        }
    },
    "query": {
        "bool": {
            "must": [
                {
                    "prefix": {
                        "categoriesMap": "{{categoriesMap}}"
                    }
                }
            ],
            "filter": []
        }
    }
}